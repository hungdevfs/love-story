{
  "expo": {
    "name": "love-story",
    "slug": "love-story",
    "version": "1.0.1",
    "orientation": "portrait",
    "icon": "./src/assets/app-info/icon.png",
    "splash": {
      "image": "./src/assets/app-info/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/app-info/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.hungdev.js.lovestory",
      "googleServicesFile": "./google-services.json"
    },
    "web": {
      "favicon": "./src/assets/app-info/favicon.png"
    }
  }
}
